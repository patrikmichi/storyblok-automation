{
  "name": "storyblok-schema-generator",
  "version": "1.0.0",
  "type": "module",
  "description": "Schema generator for Storyblok components from Figma designs",
  "scripts": {
    "generate:schema": "tsx scripts/generate-schema.ts",
    "generate:component": "tsx scripts/generate-component.ts",
    "push:schema": "tsx scripts/push-schema.ts",
    "push:schema:n8n": "tsx scripts/push-schema-n8n.ts",
    "validate:schema": "tsx scripts/validate-schema.ts",
    "test:component": "tsx scripts/test-component.ts",
    "orchestrate": "tsx scripts/orchestrate.ts",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0"
  }
}

